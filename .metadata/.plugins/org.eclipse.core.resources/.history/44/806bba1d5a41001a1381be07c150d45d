package controller;

import java.awt.Color;
import java.awt.Graphics;

public class Ball {

	public double x,y;
	public int width,height;

	public Enemy(int x, int y) {

		this.x = x;
		this.y = y;
		this.width = 40;
		this.height = 10;
		
	}
	
	public void tick() {
		
	}
	
		
	public void render(Graphics g) {
		
		g.setColor(Color.BLUE);
		g.fillRect((int)x, (int)y, width, height);
		
	}
	
	
}
