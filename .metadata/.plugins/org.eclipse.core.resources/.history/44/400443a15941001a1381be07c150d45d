package controller;

import java.awt.Color;
import java.awt.Graphics;

public class Enemy {

	public double x,y;
	public int width,height;

	public Enemy(int x, int y) {

		this.x = x;
		this.y = y;
		
	}
	
	public void tick() {
		
	}
	
		
	public void render(Graphics g) {
		
		g.setColor(Color.BLUE);
		g.fillRect(x, y, width, height);
		
	}
	
	
	
}
